package Day05;

public class _03_multiInheritOfInterface {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//자유게시판 객체 생성
		FreeBoard freeBoard = new FreeBoard();
		
		//자유게시판의 게시판 기능 사용
		Board board = freeBoard;
		board.registBoard();
		board.updateBoard();
		board.deleteBoard();
		board.getBoardList();
	}

}
